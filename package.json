    "@angular/upgrade": "^5.2.5",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.3.1",
    "@angular/compiler-cli": "^5.2.5",
    "@ngtools/webpack": "^1.9.8",
