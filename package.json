    "@angular/upgrade": "^5.0.5",
    "core-js": "^2.5.1",
    "rxjs": "^5.5.3",
    "tslib": "^1.8.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.35",
    "@angular/compiler-cli": "^5.0.5",
    "@ngtools/webpack": "^1.8.5",
