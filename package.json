{
  "name": "ng2-start",
  "version": "0.0.0",
  "private": true,
  "author": "Felix Itzenplitz",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "clean": "rimraf dist",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.dev.js --progress --colors --display-error-details --display-cached",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.prod.js --progress --colors --display-error-details --display-cached",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run lint",
    "sync": "cp -r config webpack.config.js package.json"
  },
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/http": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "@angular/router": "^3.0.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/node": "^6.0.41",
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.5.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1"
  }
}
