{
  "name": "ng2-start",
  "version": "0.0.0",
  "private": true,
  "author": "Felix Itzenplitz",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "clean": "rimraf dist",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.dev.js --progress --colors --display-error-details --display-cached",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.prod.js --progress --colors --display-error-details --display-cached",
    "lint": "tslint \"src/**/*.ts\"",
    "typings": "typings",
    "typings-upgrade": "typings install -SG dt~core-js && typings install -SG dt~node",
    "postinstall": "typings install",
    "sync": "cp -r config webpack.config.js package.json"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "html-webpack-plugin": "^2.18.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  }
}
