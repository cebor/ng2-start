    "@angular/router": "^4.3.1",
    "@angular/upgrade": "^4.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.1",
    "@ngtools/webpack": "^1.5.4",
